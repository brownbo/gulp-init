extends ../layouts/_default.pug
block meta
  - var page_name = "登録"
  title
    =page_name
  meta(
  name='keywords'
  content='keywords'
  )
  meta(
  name='description'
  content='description'
  )
  meta(
  name='language'
  content='en'
  )
block page_option
  - page_class.push('page_a page_news_list page_login')
  - base_path = '../'
  - headerType = '2'
block content
  .page_content.x_page_content
    .x_scroll_content.scroll_content
      .login_cont_wrap
        h2.title_h 登録完了
        div.login_suc_text
          p ご登録ありがとうございます。
          p TOPページより日本放題をお楽しみください。
        .login_submit_button
          a(href="/") 日本放題TOPへ