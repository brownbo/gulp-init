extends ../layouts/_default.pug
block meta
  - var page_name = "privacy policy"
  title
    =page_name
  meta(
  name='keywords'
  content='keywords'
  )
  meta(
  name='description'
  content='description'
  )
  meta(
  name='language'
  content=''
  )

block page_option
  - page_class.push('page_a page_static')
  - base_path = '../'

block content
  .page_content
    .wrapper
      section.content
        != data_privacyPolicy.content